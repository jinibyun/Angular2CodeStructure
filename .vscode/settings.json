// Place your settings in this file to overwrite default and user settings.
{
    // Controls the font size.
	"editor.fontSize": 12,
    "editor.wrappingColumn": 0,
    "editor.renderWhitespace": true,
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/.vscode": false,
        "**/node_modules": true,
        "**/publish": true,
        "**/typings": true,
        "**/*.js": false,
        "**/*.js.map": false
    }
}